  "version": "0.2.0",
  "configurations": [
    {
      "name": "consumer",
      "program": "${workspaceFolder}/consumer.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "producer",
      "program": "${workspaceFolder}/producer.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "client-printer",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start"]
    }
  ]
}
